        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"username\": \"testuser\",\n    \"password\": \"Password123!\"\n}"
            },
            "url": {
              "raw": "http://localhost:8081/v1/app/auth/login",
              "host": ["localhost"],
              "port": "8081",
              "path": ["v1", "app", "auth", "login"]
            },
            "event": [
              {
                "listen": "test",
                "script": {
                  "type": "text/javascript",
                  "exec": [
                    "var jsonData = pm.response.json();",
                    "pm.variables.set(\"auth_token\", \"Bearer \" + jsonData.token);",
                    "console.log(\"Auth token set: \" + jsonData.token);"
                  ]
                }
              }
            ]
          },
          "response": [
            {
              "name": "Login Response",
              "originalRequest": {
