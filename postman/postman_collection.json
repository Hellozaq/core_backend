                ],
                "body": {
                  "mode": "raw",
                  "raw": "{\n    \"username\": \"testuser\",\n    \"password\": \"Password123!\"\n}"
                }
              },
              "status": "OK",
              "code": 200,
              "header": [
                {
                  "key": "Content-Type",
                  "value": "application/json"
                }
              ],
              "body": "{\n    \"token\": \"eyJhbGciOiJIUzUxMiJ9...\",\n    \"user\": {\n        \"id\": 1,\n        \"username\": \"testuser\",\n        \"type\": 1,\n        \"createdAt\": [2025,4,7,18,49,59],\n        \"updatedAt\": [2025,4,7,18,49,59],\n        \"isEmailVerified\": false,\n        \"person\": {\n            \"id\": 1,\n            \"firstName\": \"John\",\n            \"lastName\": \"Doe\",\n            \"documentNumber\": \"12345678A\",\n            \"phoneNumber\": \"123456789\",\n            \"email\": \"john.doe@example.com\"\n        }\n    }\n}"
            }
          ]
        }
